{
 el=document.getElementById(ID);
 oldNo=parseInt(el.src.substring(el.src.lastIndexOf('-')+1));
 if (No!=oldNo)
 {
         col=document.getElementsByName(ID+No);
         for (i=0;i<col.length;i++) col[i].src='intr/numbers/'+No+'s.png';
         col=document.getElementsByName(ID+oldNo);
         for (i=0;i<col.length;i++) col[i].src='intr/numbers/'+oldNo+'.png';
 }
 var Path=el.src.substring(0,el.src.lastIndexOf('-')+1)+No+'.png';
eval(" if (typeof "+ID+"enlarged == 'undefined') t=0; else t="+ID+"enlarged; ");
if (t) { if (Width>0) el.style.width=(Width*2)+'px';
 eval("if (Height>0) { if (typeof "+ID+"aspect != 'undefined') el.style.height=("+ID+"aspect*Height*2)+'px'; else el.style.height=(Height*2)+'px'; }");
} else{ if (Width>0) el.style.width=Width+'px';
 eval("if (Height>0) { if (typeof "+ID+"aspect != 'undefined') el.style.height="+ID+"aspect*Height+'px'; else el.style.height=Height+'px'; }");
} el.src=Path;
}
</script><td width=36 style='padding-left: 7px' align=center valign=top><img src='intr/blank.gif' width=36 height=1><a hidefocus align=absbottom href="javascript:switchView(1,'gnome220redhat9settingsaccessibility',401,574)"><img name=gnome220redhat9settingsaccessibility1 alt='Switch to view 1' title='Switch to view 1' border=0 width=17 height=14 src='intr/numbers/1s.png'></a><br><a hidefocus align=absbottom href="javascript:switchView(2,'gnome220redhat9settingsaccessibility',401,574)"><img name=gnome220redhat9settingsaccessibility2 alt='Switch to view 2' title='Switch to view 2' border=0 width=17 height=14 src='intr/numbers/2.png'></a><br><a hidefocus align=absbottom href="javascript:switchView(3,'gnome220redhat9settingsaccessibility',401,574)"><img name=gnome220redhat9settingsaccessibility3 alt='Switch to view 3' title='Switch to view 3' border=0 width=17 height=14 src='intr/numbers/3.png'></a></td><td valign=bottom><img class=sc style='width:401px; height:574px;' id=gnome220redhat9settingsaccessibility alt='Accessibility in GNOME 2.2.0 in RedHat 9 (Keyboard Accessibility Preferences)' src='pics/gui/settings/accessibility/gnome220redhat9-1-1.png'><br><font class=imagecaption>Accessibility in GNOME 2.2.0 in RedHat 9 <b>(Keyboard Accessibility Preferences)</b></font></td></tr></table></p></td><td height=50 style='background: url(intr/3rightfill.png)'><img width=12 height=1 src='intr/3rightfill.png'></td></tr></table></td></tr><tr><td><table cellpadding=0 cellspacing=0 width=100%><tr><td height=30><img width=20 height=30 hspace=0 src='intr/3bottomleft.png'></td><td height=30 width=100% style='background: url(intr/3bottomfill.png)'></td><td height=30><img width=30 height=30 hspace=0 src='intr/3bottomclosed.png'></td></tr></table></td></tr></table><br><br><table width=100% cellpadding=0 cellspacing=0><tr><td><table cellpadding=0 cellspacing=0 width=100%><tr height=20><td valign=top width=36 rowspan=2 style='background: url(intr/3leftfill.png) white left repeat-y'><img b