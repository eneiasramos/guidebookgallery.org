{
 el=document.getElementById(ID);
 oldNo=parseInt(el.src.substring(el.src.lastIndexOf('-')+1));
 if (No!=oldNo)
 {
         col=document.getElementsByName(ID+No);
         for (i=0;i<col.length;i++) col[i].src='intr/numbers/'+No+'s.png';
         col=document.getElementsByName(ID+oldNo);
         for (i=0;i<col.length;i++) col[i].src='intr/numbers/'+oldNo+'.png';
 }
 var Path=el.src.substring(0,el.src.lastIndexOf('-')+1)+No+'.png';
eval(" if (typeof "+ID+"enlarged == 'undefined') t=0; else t="+ID+"enlarged; ");
if (t) { if (Width>0) el.style.width=(Width*2)+'px';
 eval("if (Height>0) { if (typeof "+ID+"aspect != 'undefined') el.style.height=("+ID+"aspect*Height*2)+'px'; else el.style.height=(Height*2)+'px'; }");
} else{ if (Width>0) el.style.width=Width+'px';
 eval("if (Height>0) { if (typeof "+ID+"aspect != 'undefined') el.style.height="+ID+"aspect*Height+'px'; else el.style.height=Height+'px'; }");
} el.src=Path;
}
</script><td width=36 style='padding-left: 7px' align=center valign=top><img src='intr/blank.gif' width=36 height=1><a hidefocus align=absbottom href="javascript:switchView(1,'cde15solaris9desktopempty',1024,768)"><img name=cde15solaris9desktopempty1 alt='Switch to view 1' title='Switch to view 1' border=0 width=17 height=14 src='intr/numbers/1s.png'></a><br><a hidefocus align=absbottom href="javascript:switchView(2,'cde15solaris9desktopempty',1024,768)"><img name=cde15solaris9desktopempty2 alt='Switch to view 2' title='Switch to view 2' border=0 width=17 height=14 src='intr/numbers/2.png'></a><br><a hidefocus align=absbottom href="javascript:switchView(3,'cde15solaris9desktopempty',1024,768)"><img name=cde15solaris9desktopempty3 alt='Switch to view 3' title='Switch to view 3' border=0 width=17 height=14 src='intr/numbers/3.png'></a><br><a hidefocus align=absbottom href="javascript:switchView(4,'cde15solaris9desktopempty',1024,768)"><img name=cde15solaris9desktopempty4 alt='Switch to view 4' title='Switch to view 4' border=0 width=17 height=14 src='intr/numbers/4.png'></a></td><td valign=bottom><img class=screenshot style='width:1024px; height:768px;' id=cde15solaris9desktopempty alt='Empty desktop in CDE 1.5 in Solaris 9' src='pics/gui/desktop/empty/cde15solaris9-1-1.png'><br><font class=imagecaption>Empty desktop in CDE 1.5 in Solaris 9. The screenshot has an <a title='Click to explain why' href='javascript:showTooltip("blackborder")'>extra border</a> <span id='cde15solaris9desktopemptyremoveborder'>(<a href='javascript:removeBorder("cde15solaris9desktopempty")'>remove</a>)</span><span id='cde15solaris9desktopemptyshowborder' style='display: none'>(<a href='javascript:showBorder("cde15solaris9desktopempty")'>show</a>)</span>.</font></td></tr></table></p></td><td height=50 style='background: url(intr/3rightfill.png)'><img width=12 height=1 src='intr/3rightfill.png'></td></tr></table></td></tr><tr><td><table cellpadding=0 cellspacing=0 width=100%><tr><td height=30><