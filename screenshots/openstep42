{
 el=document.getElementById(ID);
 oldNo=parseInt(el.src.substring(el.src.lastIndexOf('-')+1));
 if (No!=oldNo)
 {
         col=document.getElementsByName(ID+No);
         for (i=0;i<col.length;i++) col[i].src='intr/numbers/'+No+'s.png';
         col=document.getElementsByName(ID+oldNo);
         for (i=0;i<col.length;i++) col[i].src='intr/numbers/'+oldNo+'.png';
 }
 var Path=el.src.substring(0,el.src.lastIndexOf('-')+1)+No+'.png';
eval(" if (typeof "+ID+"enlarged == 'undefined') t=0; else t="+ID+"enlarged; ");
if (t) { if (Width>0) el.style.width=(Width*2)+'px';
 eval("if (Height>0) { if (typeof "+ID+"aspect != 'undefined') el.style.height=("+ID+"aspect*Height*2)+'px'; else el.style.height=(Height*2)+'px'; }");
} else{ if (Width>0) el.style.width=Width+'px';
 eval("if (Height>0) { if (typeof "+ID+"aspect != 'undefined') el.style.height="+ID+"aspect*Height+'px'; else el.style.height=Height+'px'; }");
} el.src=Path;
}
</script><td width=36 style='padding-left: 7px' align=center valign=top><img src='intr/blank.gif' width=36 height=1><a hidefocus align=absbottom href="javascript:switchView(1,'openstep42applicationsofficetexteditor',513,452)"><img name=openstep42applicationsofficetexteditor1 alt='Switch to view 1' title='Switch to view 1' border=0 width=17 height=14 src='intr/numbers/1s.png'></a><br><a hidefocus align=absbottom href="javascript:switchView(2,'openstep42applicationsofficetexteditor',832,717)"><img name=openstep42applicationsofficetexteditor2 alt='Switch to view 2' title='Switch to view 2' border=0 width=17 height=14 src='intr/numbers/2.png'></a></td><td valign=bottom><img class=sc style='width:513px; height:452px;' id=openstep42applicationsofficetexteditor alt='Text editor in OPENSTEP 4.2 (Edit)' src='pics/gui/applications/office/texteditor/openstep42-1-1.png'><br><font class=imagecaption>Text editor in OPENSTEP 4.2 <b>(Edit)</b></font></td></tr></table></p><br><table width=100% cellpadding=0 cellspacing=0><tr><td width=50% height=8 style='background: url(intr/divleftfill.png)'><img width=1 height=8 src='intr/divleftfill.png'></td><td width=8 height=8><img width=8 height=8 src='intr/divmed.png'></td><td width=50% height=8 style='background: url(intr/divrightfill.png)'><img width=1 height=8 src='intr/divrightfill.png'></td></tr></table><br><p><a name='calculator'></a><img style='margin-right: 8px' align=baseline src='intr/3rtarrow.png'><a title='Show how this component looks in other interfaces' href='screenshots/calculator'><big><b>Calculator</b></big></a><br><a name='calendar'></a><img style='margin-right: 8px' align=baseline src='intr/3rtarrow.png'><a title='Show how this component looks in other interfaces' href='screenshots/calendar'><big><b>Calendar</b></big></a><br></p><p><table class=screenshot cellpadding=0 cellspacing=0><tr><td width=48></td><td valign=top><br>This GUI doesn&rsquo;t have  these features.</td></tr></table></p><br><table width=100% cellpadding=